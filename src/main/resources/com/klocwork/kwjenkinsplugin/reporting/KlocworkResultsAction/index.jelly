<?jelly escape-by-default='true'?>

<j:jelly
        xmlns:j="jelly:core"
        xmlns:st="jelly:stapler"
        xmlns:l="/lib/layout"
>
    <l:layout norefresh="true">
        <l:main-panel>
            <j:set var="expandIssues" value="true"/>
            <h1>${%Klocwork Analysis Results}</h1>
            <table style="margin-top: 1em;">
                <st:include it="${it}" page="summary.jelly"/>
            </table>
        </l:main-panel>
    </l:layout>
</j:jelly>
